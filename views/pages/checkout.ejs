        <div class="infos">
            <h2>Informasi Kontak</h2>
            <fieldset>
                <label for="first-name">Nama depan: 
                    <input name="first-name" type="text" required /></label>
                <label for="last-name">Nama belakang: 
                    <input name="last-name" type="text" required /></label>
